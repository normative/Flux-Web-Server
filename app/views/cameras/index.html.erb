<h1>Listing cameras</h1>

<table>
  <tr>
    <th>User</th>
    <th>Model</th>
    <th>Deviceid</th>
    <th>Description</th>
    <th>Nickname</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @cameras.each do |camera| %>
  <tr>
    <td><%= camera.user %></td>
    <td><%= camera.model %></td>
    <td><%= camera.deviceid %></td>
    <td><%= camera.description %></td>
    <td><%= camera.nickname %></td>
    <td><%= link_to 'Show', camera %></td>
    <td><%= link_to 'Edit', edit_camera_path(camera) %></td>
    <td><%= link_to 'Destroy', camera, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Camera', new_camera_path %>
